package reportes;

/**
 *
 * @author usuario
 */
public class Reporte {
    private int idReporte;
    private String reporte;
    private String query;
    private String observaciones;
    private String titulo1;
    private String titulo2;
    private String titulo3;
    private String titulo4;
    private String titulo5;
    private String fecha;
    private int idEstatus;
    private boolean reporteSeleccionado;

    public Reporte() {
    }

    public Reporte(int idReporte, String reporte, String query, String observaciones, String titulo1, String titulo2, String titulo3, String titulo4, String titulo5, int idEstatus) {
        this.idReporte = idReporte;
        this.reporte = reporte;
        this.query = query;
        this.observaciones = observaciones;
        this.titulo1 = titulo1;
        this.titulo2 = titulo2;
        this.titulo3 = titulo3;
        this.titulo4 = titulo4;
        this.titulo5 = titulo5;
        this.idEstatus = idEstatus;
    }

    
    public int getIdReporte() {
        return idReporte;
    }

    public void setIdReporte(int idReporte) {
        this.idReporte = idReporte;
    }

    public String getReporte() {
        return reporte;
    }

    public void setReporte(String reporte) {
        this.reporte = reporte != null ? reporte.toUpperCase(): reporte;
    }

    public String getQuery() {
        return query;
    }

    public void setQuery(String query) {
        this.query = query != null ? query.toUpperCase(): query;
        
    }

    public String getObservaciones() {
        return observaciones;
    }

    public void setObservaciones(String observaciones) {
        this.observaciones = observaciones;
    }

    public int getIdEstatus() {
        return idEstatus;
    }

    public void setIdEstatus(int idEstatus) {
        this.idEstatus = idEstatus;
    }
    

    public boolean isReporteSeleccionado() {
        return reporteSeleccionado;
    }

    public void setReporteSeleccionado(boolean reporteSeleccionado) {
        this.reporteSeleccionado = reporteSeleccionado;
    }

    public String getTitulo1() {
        return titulo1;
    }

    public void setTitulo1(String titulo1) {
        this.titulo1 = titulo1 != null ? titulo1.toUpperCase(): titulo1;
    }

    public String getTitulo2() {
        return titulo2;
    }

    public void setTitulo2(String titulo2) {
        this.titulo2 = titulo2 != null ? titulo2.toUpperCase(): titulo2;
    }

    public String getTitulo3() {
        return titulo3;
    }

    public void setTitulo3(String titulo3) {
        this.titulo3 = titulo3 != null ? titulo3.toUpperCase(): titulo3;
    }

    public String getTitulo4() {
        return titulo4;
    }

    public void setTitulo4(String titulo4) {
        this.titulo4 = titulo4 != null ? titulo4.toUpperCase(): titulo4;
    }

    public String getTitulo5() {
        return titulo5;
    }

    public void setTitulo5(String titulo5) {
        this.titulo5 = titulo5 != null ? titulo5.toUpperCase(): titulo5;
    }

    public String getFecha() {
        return fecha;
    }

    public void setFecha(String fecha) {
        this.fecha = fecha;
    }
    
    
}
