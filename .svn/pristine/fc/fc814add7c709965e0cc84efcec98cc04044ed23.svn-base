	<queryString>
		<![CDATA[SELECT count(*) TOTAL,pa.Dependencia,
  de.Domicilio,
  de.NomDepto,
  pe1.Ap1
  || ' '
  || pe1.Ap2
  || ' '
  || pe1.Nombre Nombre,
  pe2.Ap1
  || ' '
  || pe2.Ap2
  || ' '
  || pe2.Nombre NombreEnlace,
  pe.Ap1
  || ' '
  || pe.Ap2
  || ' '
  || PE.NOMBRE NOMBREENCARGADO,
  PA.FUNCION
FROM Mobiliario MO,
  PARAMETROS PA
LEFT OUTER JOIN Personal PE
ON pe.NoPersonalBM = pa.NoPersonalBM
AND pe.IdEstatus   = 1,
  Departamento DE
LEFT OUTER JOIN Personal PE1
ON pe1.NoPersonalBM = de.NoPersonalBM
AND pe1.IdEstatus   = 1
LEFT OUTER JOIN Personal PE2
ON pe2.NoPersonalBM    = de.NoPersonalBM_Enlace
AND pe2.IdEstatus      = 1
WHERE mo.IdEstatus    IN (1,3)
AND de.IdDepartamento  = mo.IdDepartamento
AND PA.IDPARAMETRO     = 1
AND MO.IDDEPARTAMENTO   = $P{IDDEPARTAMENTO}
GROUP BY pa.Dependencia,
  DE.DOMICILIO,
  DE.NOMDEPTO,
  pe1.Ap1
  || ' '
  || pe1.Ap2
  || ' '
  || pe1.Nombre,
  pe2.Ap1
  || ' '
  || pe2.Ap2
  || ' '
  || pe2.Nombre,
  pe.Ap1
  || ' '
  || pe.Ap2
  || ' '
  || PE.NOMBRE,
  PA.FUNCION]]>
	</queryString>
